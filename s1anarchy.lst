000014DA                            ; Part of the Nemesis decompressor, builds the code table (in RAM)
000014DA                            ; ---------------------------------------------------------------------------
000014DA                            
000014DA                            
000014DA                            NemDec_BuildCodeTable:
000014DA 1018                       		move.b	(a0)+,d0	; read first byte
000014DC                            
000014DC                            NemBCT_ChkEnd:
000014DC 0C00 00FF                  		cmpi.b	#$FF,d0	; has the end of the code table description been reached?
000014E0 6600                       		bne.s	NemBCT_NewPALIndex	; if not, branch
000014E2 4E75                       		rts	; otherwise, this subroutine's work is done
000014E4                            ; ===========================================================================
000014E4                            
000014E4                            NemBCT_NewPALIndex:
000014E4 3E00                       		move.w	d0,d7
000014E6                            
000014E6                            NemBCT_Loop:
000014E6 1018                       		move.b	(a0)+,d0	; read next byte
000014E8 0C00 0080                  		cmpi.b	#$80,d0	; sign bit being set signifies a new palette index
000014EC 64EE                       		bcc.s	NemBCT_ChkEnd	; a bmi could have been used instead of a compare and bcc
000014EE                            		
000014EE 1200                       		move.b	d0,d1
000014F0 0247 000F                  		andi.w	#$F,d7	; get palette index
000014F4 0241 0070                  		andi.w	#$70,d1	; get repeat count for palette index
000014F8 8E41                       		or.w	d1,d7	; combine the two
000014FA 0240 000F                  		andi.w	#$F,d0	; get the length of the code in bits
000014FE 1200                       		move.b	d0,d1
00001500 E149                       		lsl.w	#8,d1
00001502 8E41                       		or.w	d1,d7	; combine with palette index and repeat count to form code table entry
00001504 7208                       		moveq	#8,d1
00001506 9240                       		sub.w	d0,d1	; is the code 8 bits long?
00001508 6600                       		bne.s	NemBCT_ShortCode	; if not, a bit of extra processing is needed
0000150A 1018                       		move.b	(a0)+,d0	; get code
0000150C D040                       		add.w	d0,d0	; each code gets a word-sized entry in the table
0000150E 3387 0000                  		move.w	d7,(a1,d0.w)	; store the entry for the code
00001512 60D2                       		bra.s	NemBCT_Loop	; repeat
00001514                            ; ===========================================================================
00001514                            
00001514                            ; the Nemesis decompressor uses prefix-free codes (no valid code is a prefix of a longer code)
00001514                            ; e.g. if 10 is a valid 2-bit code, 110 is a valid 3-bit code but 100 isn't
00001514                            ; also, when the actual compressed data is processed the high bit of each code is in bit position 7
00001514                            ; so the code needs to be bit-shifted appropriately over here before being used as a code table index
00001514                            ; additionally, the code needs multiple entries in the table because no masking is done during compressed data processing
00001514                            ; so if 11000 is a valid code then all indices of the form 11000XXX need to have the same entry
00001514                            NemBCT_ShortCode:
00001514 1018                       		move.b	(a0)+,d0	; get code
00001516 E368                       		lsl.w	d1,d0	; get index into code table
00001518 D040                       		add.w	d0,d0	; shift so that high bit is in bit position 7
0000151A 7A01                       		moveq	#1,d5
0000151C E36D                       		lsl.w	d1,d5
0000151E 5345                       		subq.w	#1,d5	; d5 = 2^d1 - 1
00001520                            
00001520                            NemBCT_ShortCode_Loop:
00001520 3387 0000                  		move.w	d7,(a1,d0.w)	; store entry
00001524 5440                       		addq.w	#2,d0	; increment index
00001526 51CD FFF8                  		dbf	d5,NemBCT_ShortCode_Loop	; repeat for required number of entries
0000152A 60BA                       		bra.s	NemBCT_Loop
