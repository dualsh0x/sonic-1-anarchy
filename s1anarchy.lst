000013EC                            
000013EC                            ; ||||||||||||||| S U B R O U T I N E |||||||||||||||||||||||||||||||||||||||
000013EC                            
000013EC                            ; Nemesis decompression to VRAM
000013EC                            NemDec:
000013EC 48E7 FFDC                  		movem.l	d0-a1/a3-a5,-(sp)
000013F0 47F9 0000 0000             		lea	(NemPCD_WriteRowToVDP).l,a3	; write all data to the same location
000013F6 49F9 00C0 0000             		lea	(vdp_data_port).l,a4	; specifically, to the VDP data port
000013FC 6000                       		bra.s	NemDecMain
000013FE                            
000013FE                            ; ||||||||||||||| S U B R O U T I N E |||||||||||||||||||||||||||||||||||||||
000013FE                            
000013FE                            ; Nemesis decompression subroutine, decompresses art to RAM
000013FE                            ; Inputs:
000013FE                            ; a0 = art address
000013FE                            ; a4 = destination RAM address
000013FE                            NemDecToRAM:
000013FE 48E7 FFDC                  		movem.l	d0-a1/a3-a5,-(sp)
00001402 47F9 0000 0000             		lea	(NemPCD_WriteRowToRAM).l,a3 ; advance to the next location after each write
00001408                            
00001408                            NemDecMain:
00001408 43F8 AA00                  		lea	(v_ngfx_buffer).w,a1
0000140C 3418                       		move.w	(a0)+,d2	; get number of patterns
0000140E E34A                       		lsl.w	#1,d2
00001410 6400                       		bcc.s	loc_146A	; branch if the sign bit isn't set
00001412 D6FC 0000                  		adda.w	#NemPCD_WriteRowToVDP_XOR-NemPCD_WriteRowToVDP,a3	; otherwise the file uses XOR mode
00001416                            
00001416                            loc_146A:
00001416 E54A                       		lsl.w	#2,d2	; get number of 8-pixel rows in the uncompressed data
00001418 3A42                       		movea.w	d2,a5	; and store it in a5 because there aren't any spare data registers
0000141A 7608                       		moveq	#8,d3	; 8 pixels in a pattern row
0000141C 7400                       		moveq	#0,d2
0000141E 7800                       		moveq	#0,d4
00001420 6100 0000                  		bsr.w	NemDec_BuildCodeTable
00001424 1A18                       		move.b	(a0)+,d5	; get first byte of compressed data
00001426 E145                       		asl.w	#8,d5	; shift up by a byte
00001428 1A18                       		move.b	(a0)+,d5	; get second byte of compressed data
0000142A 3C3C 0010                  		move.w	#$10,d6	; set initial shift value
0000142E 6100                       		bsr.s	NemDec_ProcessCompressedData
00001430 4CDF 3BFF                  		movem.l	(sp)+,d0-a1/a3-a5
00001434 4E75                       		rts	
