00001436                            ; ---------------------------------------------------------------------------
00001436                            ; Part of the Nemesis decompressor, processes the actual compressed data
00001436                            ; ---------------------------------------------------------------------------
00001436                            
00001436                            ; ||||||||||||||| S U B	R O U T	I N E |||||||||||||||||||||||||||||||||||||||
00001436                            
00001436                            
00001436                            NemDec_ProcessCompressedData:
00001436 3E06                       		move.w	d6,d7
00001438 5147                       		subq.w	#8,d7	; get shift value
0000143A 3205                       		move.w	d5,d1
0000143C EE69                       		lsr.w	d7,d1	; shift so that high bit of the code is in bit position 7
0000143E 0C01 00FC                  		cmpi.b	#%11111100,d1	; are the high 6 bits set?
00001442 6400                       		bcc.s	NemPCD_InlineData	; if they are, it signifies inline data
00001444 0241 00FF                  		andi.w	#$FF,d1
00001448 D241                       		add.w	d1,d1
0000144A 1031 1000                  		move.b	(a1,d1.w),d0	; get the length of the code in bits
0000144E 4880                       		ext.w	d0
00001450 9C40                       		sub.w	d0,d6	; subtract from shift value so that the next code is read next time around
00001452 0C46 0009                  		cmpi.w	#9,d6	; does a new byte need to be read?
00001456 6400                       		bcc.s	loc_14B2	; if not, branch
00001458 5046                       		addq.w	#8,d6
0000145A E145                       		asl.w	#8,d5
0000145C 1A18                       		move.b	(a0)+,d5	; read next byte
0000145E                            
0000145E                            loc_14B2:
0000145E 1231 1001                  		move.b	1(a1,d1.w),d1
00001462 3001                       		move.w	d1,d0
00001464 0241 000F                  		andi.w	#$F,d1	; get palette index for pixel
00001468 0240 00F0                  		andi.w	#$F0,d0
0000146C                            
0000146C                            NemPCD_ProcessCompressedData:
0000146C E848                       		lsr.w	#4,d0	; get repeat count
0000146E                            
0000146E                            NemPCD_WritePixel:
0000146E E98C                       		lsl.l	#4,d4	; shift up by a nybble
00001470 8801                       		or.b	d1,d4	; write pixel
00001472 5343                       		subq.w	#1,d3	; has an entire 8-pixel row been written?
00001474 6600                       		bne.s	NemPCD_WritePixel_Loop	; if not, loop
00001476 4ED3                       		jmp	(a3)	; otherwise, write the row to its destination, by doing a dynamic jump to NemPCD_WriteRowToVDP, NemDec_WriteAndAdvance, NemPCD_WriteRowToVDP_XOR, or NemDec_WriteAndAdvance_XOR
00001478                            ; End of function NemDec_ProcessCompressedData
00001478                            
00001478                            
00001478                            ; ||||||||||||||| S U B	R O U T	I N E |||||||||||||||||||||||||||||||||||||||
00001478                            
00001478                            
00001478                            NemPCD_NewRow:
00001478 7800                       		moveq	#0,d4	; reset row
0000147A 7608                       		moveq	#8,d3	; reset nybble counter
0000147C                            
0000147C                            NemPCD_WritePixel_Loop:
0000147C 51C8 FFF0                  		dbf	d0,NemPCD_WritePixel
00001480 60B4                       		bra.s	NemDec_ProcessCompressedData
00001482                            ; ===========================================================================
00001482                            
00001482                            NemPCD_InlineData:
00001482 5D46                       		subq.w	#6,d6	; 6 bits needed to signal inline data
00001484 0C46 0009                  		cmpi.w	#9,d6
00001488 6400                       		bcc.s	loc_14E4
0000148A 5046                       		addq.w	#8,d6
0000148C E145                       		asl.w	#8,d5
0000148E 1A18                       		move.b	(a0)+,d5
00001490                            
00001490                            loc_14E4:
00001490 5F46                       		subq.w	#7,d6	; and 7 bits needed for the inline data itself
00001492 3205                       		move.w	d5,d1
00001494 EC69                       		lsr.w	d6,d1	; shift so that low bit of the code is in bit position 0
00001496 3001                       		move.w	d1,d0
00001498 0241 000F                  		andi.w	#$F,d1	; get palette index for pixel
0000149C 0240 0070                  		andi.w	#$70,d0	; high nybble is repeat count for pixel
000014A0 0C46 0009                  		cmpi.w	#9,d6
000014A4 64C6                       		bcc.s	NemPCD_ProcessCompressedData
000014A6 5046                       		addq.w	#8,d6
000014A8 E145                       		asl.w	#8,d5
000014AA 1A18                       		move.b	(a0)+,d5
000014AC 60BE                       		bra.s	NemPCD_ProcessCompressedData
000014AE                            ; End of function NemPCD_NewRow
000014AE                            
000014AE                            ; ===========================================================================
000014AE                            
000014AE                            NemPCD_WriteRowToVDP:
000014AE 2884                       		move.l	d4,(a4)	; write 8-pixel row
000014B0 534D                       		subq.w	#1,a5
000014B2 380D                       		move.w	a5,d4	; have all the 8-pixel rows been written?
